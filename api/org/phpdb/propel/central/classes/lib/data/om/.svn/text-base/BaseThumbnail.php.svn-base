	 */
	public function toArray($keyType = BasePeer::TYPE_PHPNAME)
	{
		$keys = ThumbnailPeer::getFieldNames($keyType);
		$result = array(
			$keys[0] => $this->getId(),
			$keys[1] => $this->getDataFileId(),
			$keys[2] => $this->getEntityId(),
			$keys[3] => $this->getEntityTypeId(),
