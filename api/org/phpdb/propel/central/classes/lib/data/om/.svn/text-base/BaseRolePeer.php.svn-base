		BasePeer::TYPE_COLNAME => array (RolePeer::ID => 0, RolePeer::DEFAULT_PERMISSIONS => 1, RolePeer::ENTITY_TYPE_ID => 2, RolePeer::DISPLAY_NAME => 3, RolePeer::SYSTEM_NAME => 4, RolePeer::SUPER_ROLE => 5, ),
