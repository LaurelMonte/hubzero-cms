		BasePeer::TYPE_FIELDNAME => array ('ID', 'DEFAULT_PERMISSIONS', 'ENTITY_TYPE_ID', 'DISPLAY_NAME', 'SYSTEM_NAME', 'SUPER_ROLE', ),
