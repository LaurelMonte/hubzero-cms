	/**
	 * Selects a collection of Role objects pre-filled with their EntityType objects.
	 *
	 * @return     array Array of Role objects.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	public static function doSelectJoinEntityType(Criteria $c, $con = null)
	{
		$c = clone $c;

		// Set the correct dbName if it has not been overridden
		if ($c->getDbName() == Propel::getDefaultDB()) {
			$c->setDbName(self::DATABASE_NAME);
		}

		RolePeer::addSelectColumns($c);
		$startcol = (RolePeer::NUM_COLUMNS - RolePeer::NUM_LAZY_LOAD_COLUMNS) + 1;
		EntityTypePeer::addSelectColumns($c);

		$c->addJoin(RolePeer::ENTITY_TYPE_ID, EntityTypePeer::ID);
		$rs = BasePeer::doSelect($c, $con);
		$results = array();

		while($rs->next()) {

			$omClass = RolePeer::getOMClass();

			$cls = Propel::import($omClass);
			$obj1 = new $cls();
			$obj1->hydrate($rs);

			$omClass = EntityTypePeer::getOMClass();

			$cls = Propel::import($omClass);
			$obj2 = new $cls();
			$obj2->hydrate($rs, $startcol);

			$newObject = true;
			foreach($results as $temp_obj1) {
				$temp_obj2 = $temp_obj1->getEntityType(); //CHECKME
				if ($temp_obj2->getPrimaryKey() === $obj2->getPrimaryKey()) {
					$newObject = false;
					// e.g. $author->addBookRelatedByBookId()
					$temp_obj2->addRole($obj1); //CHECKME
					break;
				}
			}
			if ($newObject) {
				$obj2->initRoles();
				$obj2->addRole($obj1); //CHECKME
			}
			$results[] = $obj1;
		}
		return $results;
	}


