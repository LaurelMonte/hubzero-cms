<?xml version="1.0" encoding="UTF-8"?>
<install type="component" version="1.5.0">
 <name>My Projects</name>
 <!-- The following elements are optional and free of formatting constraints -->
 <creationDate>2010-07-15</creationDate>
 <author>Gemez Marshall</author>
 <authorEmail>gemezm@purdue.edu</authorEmail>
 <authorUrl></authorUrl>
 <copyright></copyright>
 <license></license>
 <!--  The version string is recorded in the components table -->
 <version>1.0.0</version>
 <!-- The description is optional and defaults to the name -->
 <description>Displays the projects belonging to a logged in user.</description>
 
 <!-- Site Main File Copy Section -->
 <!-- Note the folder attribute: This attribute describes the folder
      to copy FROM in the package to install therefore files copied
      in this section are copied from /site/ in the package -->
 <files folder="site">
  <filename>controller.php</filename>
  <filename>myprojects.php</filename>
  <filename>index.html</filename>
  <filename>models/get.php</filename>
  <filename>models/index.html</filename>
  <filename>views/index.html</filename>
  <filename>views/get/index.html</filename>
  <filename>views/get/view.html.php</filename>
  <filename>views/get/tmpl/default.php</filename>
  <filename>views/get/tmpl/index.html</filename>
 </files>
 
 <administration>
  <!-- Administration Menu Section -->
  <menu>My Projects</menu>
 
  <!-- Administration Main File Copy Section -->
  <files folder="admin">
   <filename>controller.php</filename>
   <filename>myprojects.php</filename>
   <filename>index.html</filename>
   <filename>models/get.php</filename>
   <filename>models/index.html</filename>
   <filename>views/index.html</filename>
   <filename>views/get/index.html</filename>
   <filename>views/get/view.html.php</filename>
   <filename>views/get/tmpl/default.php</filename>
   <filename>views/get/tmpl/index.html</filename>
  </files>
  
 </administration>
</install>
